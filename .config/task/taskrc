regex=on
search.case.sensitive=no
uda.env.type=string
uda.env.label=Environment
uda.env.values=dev,bsln,nipr,ahds,sipr,ntap
uda.env.default=nipr
uda.assigned.type=string
uda.assigned.label=Assigned To
report.assigned.description=All the tasks that have been assigned to other people
report.assigned.columns=id,assigned,project,description,urgency
report.assigned.sort=assigned+
report.assigned.filter=assigned.not:
report.list.filter=assigned: and status.not:Completed and status.not:Deleted and -backlog
report.backlog.description=All of the tasks currently in the backlog
report.backlog.filter=status.not:Completed and status.not:Deleted and assigned: +backlog
report.backlog.columns=id,entry.age,due,project,description.count,tags
